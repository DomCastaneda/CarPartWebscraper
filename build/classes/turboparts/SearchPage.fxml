<?xml version="1.0" encoding="UTF-8"?>

<!--If we include fontawesome import, we can't edit fxml file
but if we exclude it, we can't run the project-->
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="664.0" prefWidth="977.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="turboparts.SearchController">
    <children>
      <HBox fx:id="outputHBox" alignment="BOTTOM_LEFT" layoutX="199.0" layoutY="602.0" prefHeight="80.0" prefWidth="784.0" styleClass="third-color" stylesheets="@style.css">
         <children>
            <Label fx:id="outputPrice" alignment="TOP_LEFT" prefHeight="111.0" prefWidth="784.0">
               <font>
                  <Font name="SansSerif Regular" size="20.0" />
               </font>
               <padding>
                  <Insets left="7.0" />
               </padding>
            </Label>
         </children>
      </HBox>
      <ComboBox fx:id="make" layoutX="224.0" layoutY="321.0" onAction="#makeAction" prefHeight="69.0" prefWidth="175.0" promptText="Select Make" styleClass="cb" stylesheets="@style.css">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </ComboBox>
      <ComboBox fx:id="year" layoutX="410.0" layoutY="321.0" onAction="#yearAction" prefHeight="69.0" prefWidth="175.0" promptText="Select Year" styleClass="cb" />
      <ComboBox fx:id="model" layoutX="596.0" layoutY="321.0" onAction="#modelAction" prefHeight="69.0" prefWidth="175.0" promptText="Select Model" styleClass="cb" />
      <ComboBox fx:id="part" layoutX="783.0" layoutY="321.0" onAction="#partAction" prefHeight="69.0" prefWidth="175.0" promptText="Select Part" styleClass="cb" />
      <VBox layoutY="119.0" prefHeight="564.0" prefWidth="199.0" styleClass="secondary-color" stylesheets="@style.css">
         <children>
            <HBox alignment="CENTER_RIGHT" prefHeight="92.0" prefWidth="199.0">
               <children>
                  <ImageView fitHeight="25.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../imgSearch.png" />
                     </image>
                  </ImageView>
                  <Button alignment="BASELINE_LEFT" graphicTextGap="12.0" mnemonicParsing="false" onAction="#switchToSearch" prefHeight="84.0" prefWidth="146.0" text="Search" textAlignment="RIGHT" textFill="WHITE">
                     <graphic>
                        <FontAwesomeIconView fill="WHITE" glyphName="SEARCH" size="24" />
                     </graphic>
                     <padding>
                        <Insets left="15.0" />
                     </padding>
                     <font>
                        <Font name="SansSerif Regular" size="20.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="90.0" prefWidth="199.0">
               <children>
                  <ImageView fitHeight="26.0" fitWidth="26.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../imgSetting.png" />
                     </image>
                  </ImageView>
                  <Button alignment="BASELINE_LEFT" graphicTextGap="12.0" mnemonicParsing="false" prefHeight="90.0" prefWidth="147.0" text="Settings" textAlignment="CENTER" textFill="WHITE">
                     <graphic>
                        <FontAwesomeIconView fill="WHITE" glyphName="GEAR" size="24" />
                     </graphic>
                     <padding>
                        <Insets left="15.0" />
                     </padding>
                     <font>
                        <Font name="SansSerif Regular" size="20.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" layoutX="10.0" layoutY="102.0" prefHeight="90.0" prefWidth="199.0">
               <children>
                  <ImageView fitHeight="26.0" fitWidth="26.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../../imgProfile.png" />
                     </image>
                  </ImageView>
                  <Button alignment="BASELINE_LEFT" graphicTextGap="12.0" mnemonicParsing="false" prefHeight="90.0" prefWidth="147.0" text="Profile" textAlignment="CENTER" textFill="WHITE">
                     <graphic>
                        <FontAwesomeIconView fill="WHITE" glyphName="GEAR" size="24" />
                     </graphic>
                     <padding>
                        <Insets left="15.0" />
                     </padding>
                     <font>
                        <Font name="SansSerif Regular" size="20.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
      <HBox alignment="CENTER_LEFT" prefHeight="118.0" prefWidth="983.0" styleClass="primary-color" stylesheets="@style.css">
         <children>
            <Label graphicTextGap="20.0" prefHeight="139.0" prefWidth="387.0" text="Turbo Parts" textFill="WHITE">
               <font>
                  <Font name="SansSerif Regular" size="40.0" />
               </font>
               <graphic>
                     </graphic>
               <padding>
                  <Insets left="10.0" />
               </padding>
               <graphic>
                  <ImageView fitHeight="124.0" fitWidth="119.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../carlogo.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Label>
         </children>
      </HBox>
        <Button fx:id="searchButton" layoutX="503.0" layoutY="425.0" onAction="#searchButtonAction" prefHeight="48.0" prefWidth="175.0" styleClass="search" stylesheets="@style.css" text="Search" textFill="WHITE">
         <font>
            <Font name="SansSerif Regular" size="30.0" />
         </font></Button>
      <HBox alignment="CENTER" layoutX="199.0" layoutY="119.0" prefHeight="80.0" prefWidth="784.0" stylesheets="@style.css">
         <children>
            <Label alignment="CENTER" contentDisplay="BOTTOM" prefHeight="69.0" prefWidth="199.0" text="Search">
               <font>
                  <Font name="SansSerif Regular" size="31.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <Hyperlink fx:id="partLink" alignment="BOTTOM_LEFT" layoutX="200.0" layoutY="643.0" onAction="#linkAction" prefHeight="40.0" prefWidth="140.0" text="Part Link">
         <font>
            <Font name="SansSerif Regular" size="20.0" />
         </font>
      </Hyperlink>
    </children>
</AnchorPane>
