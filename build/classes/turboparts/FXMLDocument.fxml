<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" prefHeight="350.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="turboparts.Controller">
    <children>
        <Button fx:id="searchButton" layoutX="294.0" layoutY="162.0" onAction="#searchButtonAction" text="Search" />
      <HBox fx:id="outputHBox" layoutX="21.0" layoutY="175.0" prefHeight="161.0" prefWidth="299.0">
         <children>
            <Label fx:id="outputPrice" prefHeight="163.0" prefWidth="301.0" text="Label" />
         </children></HBox>
      <ComboBox fx:id="make" layoutX="-2.0" layoutY="121.0" onAction="#makeAction" prefWidth="150.0" promptText="Select Make" />
      <ComboBox fx:id="year" layoutX="155.0" layoutY="121.0" onAction="#yearAction" prefWidth="150.0" promptText="Select Year" />
      <ComboBox fx:id="model" layoutX="311.0" layoutY="121.0" onAction="#modelAction" prefWidth="150.0" promptText="Select Model" />
      <ComboBox fx:id="part" layoutX="468.0" layoutY="121.0" onAction="#partAction" prefWidth="150.0" promptText="Select Part" />
      <Hyperlink fx:id="partLink" layoutX="541.0" layoutY="305.0" onAction="#linkAction" prefHeight="31.0" prefWidth="109.0" text="Website" />
    </children>
</AnchorPane>
